<h1>Works#index</h1>
<p>Find me in app/views/works/index.html.erb</p>

<% @works.each do |works| %>
	<p>
	serial: <%= works.inventory.serial %>
	<br>
	usuario: <%= works.user.name %>
	<br>
	categoria: <%= works.inventory.category.name %>
	<br>
	<%= link_to 'Delete', work_path(works), :method => :delete %>
	</p>

<% end %>
